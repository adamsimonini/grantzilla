<div class="upload" id="<%= upload.id %>">
  <div class="front">
    <%= link_to upload, class: 'pic-link' do %>
      <%= image_tag upload.file.url(:square) %>
    <% end %>
  </div>

  <div class="back">
    <span class="upload-name"><%= upload.name %></span>
    <div class="actions">
      <%= link_to 'Show', upload %>
      <%= link_to 'Edit', edit_upload_path(upload) %>
      <%= link_to 'Delete', upload, confirm: 'Are you sure you want to delete upload?', method: :delete, class: 'destroy' %>
    </div>
  </div>
</div>
